<view class='header'>
  <view class='avatar'>头像</view>
  <view class='nickname'>昵称</view>
  <view class='price'>消费金额</view>
</view>
<view class='content'>
  <block wx:for='{{list}}' wx:key='{{index}}'>
    <view class='item'>
      <image class='item-avatar' src='{{item.avatar}}'></image>
      <view class='item-nickname'>{{item.nickname}}</view>
      <view class='item-price'>{{item.money}}</view>
    </view>
  </block>
</view>