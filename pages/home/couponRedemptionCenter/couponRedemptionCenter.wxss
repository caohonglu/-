page{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-flow:row wrap;
  background-color: #eeeeee;
}
.couponRedemptionCenter{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-flow: row wrap;
}
.couponsCenter{
  height: 90vh;
  width: 600rpx;
  margin-top:-35rpx;
}
.couponsCenter-item{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-flow: column wrap;
  position: relative;
  width: 600rpx;
  height: 370rpx;
  margin-bottom: 7.5rpx;
}

.couponsCenter-item-img{
  width: 600rpx;
  height: 370rpx;
  position:absolute;
  left: 0;
  top: 0;
  z-index: 1;
}
.couponsCenter-print{
  width: 650rpx;
  height: 75rpx;
  margin-top: 75rpx;
}

.couponsCenter-item-name{
  z-index: 9;
  color: #f00;
  font-weight: 600;
  font-size: 50rpx;
}
.couponsCenter-item-price{
  z-index: 9;
  color: rgb(255, 55, 55);
  font-weight: 900;
  font-size: 70rpx;
}
.couponsCenter-item-full{
  z-index: 9;
  margin: 15rpx 0;
  font-size: 35rpx;
}
.couponsCenter-item-get{
  z-index: 9;
  color: #ee0f17;
  border: #ee0f17 1px solid;
  border-radius:50rpx;
  padding: 10rpx 50rpx;
  font-size: 30rpx;
  letter-spacing: 2rpx;
  margin-top: 7.5rpx;
}